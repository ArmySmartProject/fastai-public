<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ai.maum.biz.fastaicc.main.cousult.chatbotBuilder.chatbotMapper.ChatbotMapper">

    <select id="selectAccountListByNo" parameterType="map" resultType="map">
        <![CDATA[
			SELECT *
			FROM Account
			WHERE No IN
		]]>
        <foreach collection="botIds" item="botId" index="index" separator="," open="(" close=")">
            #{botId}
        </foreach>
        <![CDATA[
		    ORDER BY Host;
		]]>
    </select>
</mapper>
